{
    "compilerOptions": {
        "allowJs": false,
        "declaration": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "esnext"
        ],
        "module": "CommonJS",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "outDir": "./dist",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES6"
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}
